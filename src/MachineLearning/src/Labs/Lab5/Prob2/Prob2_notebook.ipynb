   "source": [
    "import numpy as np\n",
    "import pcn\n",
    "\n",
    "a = np.array([[1,1,1],[1,2,1],[2,1,1],[0,0,0],[1,0,0],[0,1,0]])\n",
    "\n",
    "x = a[:, 0:2]\n",
    "y = a[:, 2:]\n",
    "\n",
    "p = pcn.pcn(x, y)\n",
    "w = p.pcntrain(x, y, 0.25, 20)\n",
    "p.confmat(x, y)"
   ]
  },
